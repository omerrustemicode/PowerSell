<Window x:Class="PowerSell.Views.Admin.AdminDashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PowerSell.Views.Admin"
        mc:Ignorable="d"
        Title="AdminDashboard" Height="450" Width="800">
    <Grid>
        <Image Source="/Views/Logos/PowerSell_BG_Logo.jpeg" Stretch="Fill" />
        <DockPanel LastChildFill="False" Margin="0,0,663,0" Grid.Row="1" Grid.Column="50">
            <Button Content="Home" DockPanel.Dock="Top" Height="51"/>
            <Button Content="Workers" DockPanel.Dock="Top" Height="51"/>
            <Button Content="Raports" DockPanel.Dock="Top" Height="51"/>
            <Button Content="Tables" DockPanel.Dock="Top" Height="51"/>
            <Button Content="Services" DockPanel.Dock="Top" Height="51"/>
            <Button Content="Categories" DockPanel.Dock="Top" Height="51"/>
            <Button Content="Settings" DockPanel.Dock="Top" Height="51"/>
        </DockPanel>
        <DockPanel LastChildFill="False" Margin="142,0,0,0"/>
    </Grid>
</Window>
